import React from "react";

import logo from "../assets/images/common/logo.png";
import close_btn from "../assets/images/sub/close_btn.png";
import ok_icon from "../assets/images/sub/ok_icon.png";


const Deposit = props => {
    return (
        <body>
            <div id="sub">
                <div className="wrap">
                    <div className="top">
                        <h1><a href="../index.html"><img src={logo} alt="월드슬롯"/></a></h1>
                        <div className="close"><a href="../index.html"><img src={close_btn} alt="Close"/></a></div>
                    </div>
                    <ul className="tap">
                        <li><a href="../sub/sub1.html" className="on">입금신청</a></li>
                        <li><a href="../sub/sub2.html">출금신청</a></li>
                        <li><a href="../sub/sub3.html">베팅내역</a></li>
                        <li><a href="../sub/sub4.html">입출내역</a></li>
                        <li><a href="../sub/sub5.html">문의내역</a></li>
                        <li><a href="../sub/sub6.html">공지사항</a></li>
                    </ul>
                    <div className="content">
                        <table className="board-write">
                            <tbody>
                                <tr>
                                    <th>입금금액</th>
                                    <td>
                                        <input type="text" placeholder="최소입금 10,000부터" />
                                        <ul className="amount">
                                            <li><a href="#">만원</a></li>
                                            <li><a href="#">오만원</a></li>
                                            <li><a href="#">십만원</a></li>
                                            <li><a href="#">오십만원</a></li>
                                            <li><a href="#">백만원</a></li>
                                            <li><a href="#" className="reset">초기화</a></li>
                                        </ul>
                                    </td>
                                </tr>
                                <tr>
                                    <th>회원정보</th>
                                    <td><input type="text" placeholder="홍길동" /></td>
                                </tr>
                                <tr>
                                    <th>입금자명</th>
                                    <td>
                                        <input type="text" placeholder="홍길동" />
                                        <p className="care">※ 입금장명이 다른 경우 이름을 수정 후 신청 하시기 바랍니다.</p>
                                    </td>
                                </tr>
                                <tr>
                                    <th>휴대폰번호</th>
                                    <td><input type="text" placeholder="01000000000" /></td>
                                </tr>
                                <tr>
                                    <th>쿠폰선택</th>
                                    <td><select><option>쿠폰사용</option></select></td>
                                </tr>
                            </tbody>
                        </table>
                        <div className="button"><a href="#" className="on"><img src={ok_icon} alt="신청"/>입금신청</a></div>
                    </div>
                </div>
            </div>
        </body>
    );
}

export default Deposit;
